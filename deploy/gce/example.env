# ==== HongBookStore backend runtime env (GCE) ====

# --- Server ---
SPRING_PROFILES_ACTIVE=prod

# --- MySQL ---
MYSQL_HOST=127.0.0.1
MYSQL_PORT=3306
MYSQL_DB=hongBookstore
MYSQL_USERNAME=your_user
MYSQL_PASSWORD=your_password

# --- Redis ---
REDIS_HOST=127.0.0.1
REDIS_PORT=6379

# --- JWT ---
JWT_SECRET_KEY=change_me

# --- Mail ---
MAIL_USERNAME=your@gmail.com
MAIL_PASSWORD=app_password

# --- OAuth ---
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
NAVER_CLIENT_ID=
NAVER_CLIENT_SECRET=
KAKAO_CLIENT_ID=
KAKAO_CLIENT_SECRET=

# --- Kakao API ---
KAKAO_API_REST_KEY=

# --- Naver Map/Route ---
NAVER_SEARCH_ID=
NAVER_SEARCH_SECRET=
NAVER_ROUTE_ID=
NAVER_ROUTE_SECRET=

# --- Weather ---
KMA_APIHUB_KEY=

# --- GCP ---
GCP_PROJECT_ID=your-project-id
GCP_BUCKET_NAME=your-bucket
# GCP_CREDENTIALS_LOCATION left empty to use ADC on GCE
GCP_CREDENTIALS_LOCATION=

# --- CORS ---
# Comma-separated list; include your Vercel and custom domains
ALLOWED_ORIGINS=https://your-frontend.vercel.app,https://www.your-domain

